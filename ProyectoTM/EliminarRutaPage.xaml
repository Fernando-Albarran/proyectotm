<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProyectoTM.EliminarRutaPage"
             Title="EliminarRutaPage">
    <VerticalStackLayout Padding ="20" Spacing="10">
         <Label Text="Eliminar ruta por identificador" FontSize="21"/>
    
         <Entry x:Name="identificadorR" Placeholder="Identificador de la ruta a eliminar"/>
    
         <Button Text="Eliminar ruta" Clicked="EliminarRutaClick"/>
    
         
    </VerticalStackLayout>
</ContentPage>